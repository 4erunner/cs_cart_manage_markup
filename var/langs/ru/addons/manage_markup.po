msgid "price competitor will be a priority if they are above the nominal otherwise use standard pricing"
msgstr "цены конкурента будут приоритетными, если они будут выше номинальной в ином случае используется стандартное ценообразование"

msgctxt "SettingsOptions::manage_markup::manage_markup_concurent_file"
msgid "Price list competitor"
msgstr "Прайс-лист конкурента"

msgctxt "SettingsTooltips::manage_markup::manage_markup_concurent_file"
msgid "the file CSV must have a value in the form of <code of the goods; the price>"
msgstr "файл CSV должен иметь значения в виде <код товара;цена>"
