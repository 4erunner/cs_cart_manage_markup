msgctxt "SettingsOptions::manage_markup::manage_markup_logging_enable"
msgid "Enable logging"
msgstr "Включить логирование"

msgctxt "SettingsTooltips::manage_markup::manage_markup_logging_enable"
msgid "Logs will be added to the catalog: [www]/files/[company_id]/manage_markup/logs/"
msgstr "Логи будут добавлены в каталог: [www]/files/[company_id]/manage_markup/logs/"

msgctxt "SettingsOptions::manage_markup::manage_markup_round"
msgid "The fractional part of the price"
msgstr "Дробная часть цены"

msgctxt "SettingsTooltips::manage_markup::manage_markup_round"
msgid "How many decimal places to leave the price"
msgstr "Сколько знаков после запятой оставить в цене"

msgctxt "SettingsOptions::manage_markup::manage_markup_list_price"
msgid "Change the price of the recommended price"
msgstr "Заменить цену на рекомендованную цену"

msgctxt "SettingsTooltips::manage_markup::manage_markup_list_price"
msgid "Replaces the price of the recommended price, resets the value LIST PRICE"
msgstr "Заменяет цену на рекомендованную цену, обнуляет значение LIST PRICE"

msgctxt "SettingsOptions::manage_markup::manage_markup_filter_price"
